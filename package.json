{
  "name": "democracyclient",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "start:ios": "react-native run-ios",
    "start:android": "react-native run-android",
    "start:web": "react-scripts-ts start",
    "build:web": "react-scripts-ts build",
    "eject:web": "react-scripts eject",
    "tsc": "tsc",
    "clean": "rimraf build/dist",
    "build": "npm run clean && npm run tsc --",
    "watch": "npm run build -- -w",
    "start:iosts":
      "npm run build && concurrently -r \"npm run watch\" \"react-native run-ios\"",
    "start:androidts":
      "npm run build && concurrently -r \"npm run watch\" \"react-native run-android\""
  },
  "dependencies": {
    "react": "16.0.0",
    "react-dom": "^16.2.0",
    "react-native": "0.50.4",
    "react-native-web": "^0.1.16",
    "react-scripts": "1.0.17",
    "react-scripts-ts": "2.8.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/react": "^16.0.27",
    "@types/react-native": "^0.50.7",
    "awesome-typescript-loader": "^3.4.1",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "concurrently": "^3.5.1",
    "jest": "21.2.1",
    "prettier": "^1.9.0",
    "react-native-cli": "^2.0.1",
    "react-test-renderer": "16.0.0",
    "source-map-loader": "^0.2.3",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
